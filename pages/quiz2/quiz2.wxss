}

.WayPoint{
    background-image: url("data:image/jpeg;base64,");
  background-size: 100% 100%;
}


.row{
  display: flex;
  flex-direction: row;
}
.column{
  display: flex;
  flex-direction: column;
}

.row-center{
  display: flex;
  justify-content: center;
}

.divLine{
  background: #E0E3DA;
  width: 95%;
  height: 2rpx;
}

.textView {
width: 100%;
