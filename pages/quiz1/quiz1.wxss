.container{
  padding: 10rpx;
}
.container-left{
  padding-left: 5%;
}
.dd{
  background-color: rgb(226, 121, 126);
  padding: 15px;
}
.tr {
  display: flex;
  justify-content: center;
}
.td {
  padding: 10px;
  border-bottom: 2px solid rgb(0, 0, 0);
  border-right: 2px solid black;
  border-left: 2px solid black;
  border-top: 2px solid black;
  text-align: center;
  width:90rpx;
  height: 90rpx;
}


.Map{
  background-color: white
}

.Start{
  background-image: url("data:image/jpeg;base64,");
  background-size: 100% 100%;
}

.End{
  background-image: url("data:image/jpeg;base64,");
  background-size: 100% 100%;
}

.WayPoint{
  background-color: #FFDD00
}
.row{
  display: flex;
  flex-direction: row;
}
.column{
  display: flex;
  flex-direction: column;
}

.row-center{
  display: flex;
  justify-content: center;
}

.divLine{
  background: rgb(212, 151, 151);
  width: 150%;
  height: 2rpx;
}

.textView {
width: 100%;
word-break: keep-all;
word-wrap: break-word
}
.blank{
  height: 30rpx;
}

.story{
  color: rgb(112, 112, 112);
  width: 90%;
}

.hint{
  color: black; 
  font-weight: bolder;
}

.title{
  color: black;
  font-weight: bolder;
