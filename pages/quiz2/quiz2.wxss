}
.tr {
  display: flex;
  justify-content: center;
}
.td {
  padding: 10px;
  border-bottom: 2px solid black;
  border-right: 2px solid black;
  border-left: 2px solid black;
  border-top: 2px solid black;
  text-align: center;
  width:20rpx;
  height: 20rpx;
}


.Map{
  background-color: white
}

.Start{
  background-image: url("data:image/jpeg;base64,");
